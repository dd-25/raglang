{
  "name": "langgraph-rag-api",
  "version": "1.0.0",
  "description": "Production-grade LangGraph RAG application with REST API",
  "main": "server.py",
  "scripts": {
    "start": "python run.py",
    "server": "python server.py",
    "dev": "uvicorn server:app --reload --host 0.0.0.0 --port 8000",
    "dev-console": "python run.py",
    "test": "python -m pytest tests/",
    "lint": "flake8 src/",
    "format": "black src/",
    "install": "pip install -r requirements.txt"
  },
  "directories": {
    "src": "./src",
    "tests": "./tests",
    "docs": "./docs",
    "uploads": "./uploads"
  },
  "paths": {
    "@src/*": ["src/*"],
    "@interface/*": ["src/interface/*"],
    "@modules/*": ["src/modules/*"],
    "@config/*": ["src/config/*"],
    "@utils/*": ["src/utils/*"],
    "@llm/*": ["src/llm/*"],
    "@db/*": ["src/db/*"]
  },
  "api": {
    "host": "0.0.0.0",
    "port": 8000,
    "endpoints": {
      "upload": "/upload",
      "query": "/query",
      "health": "/health"
    }
  },
  "keywords": ["langgraph", "rag", "llm", "ai", "fastapi", "rest-api"],
  "author": "Your Name",
  "license": "MIT"
}
